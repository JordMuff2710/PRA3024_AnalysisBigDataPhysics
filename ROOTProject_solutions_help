    "masshist = ROOT.TH1F(\"masshist\",\"Histogram of Bs mass\",nbins,massrange[0],massrange[1])\n",
    "\n",
    "#for event in mctree :\n",
    "#  mass = event.lab0_MM\n",
    "#  masshist.Fill( mass )\n",
    "\n",
